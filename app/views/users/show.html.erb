<div class='container'>
  <h1><%= current_user.first_name %>, <%= current_user.last_name %> profil</h1>
  <p> description : <%= current_user.description%></p>
  <p> email : <%= current_user.email%></p>
  <% @event.each do |user_event|%>
    <% if user_event.user_id == current_user.id %>
      <%= link_to(user_event.title, event_path(user_event.id))%>
    <% end %>
  <% end %>
</div>